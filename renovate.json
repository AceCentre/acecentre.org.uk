{
  "extends": ["config:base"],
  "dependencyDashboard": true,
  "ignoreDeps": [
    "storybook-css-modules-preset",
    "prom-client",
    "got",
    "netlify-cli"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchPackageNames": ["@netlify/plugin-nextjs"],
      "automerge": false
    }
  ]
}
